{
  "name": "@picter/npm-package-boilerplate",
  "version": "0.1.0",
  "description": "Boilerplate to create new modules to be shared on NPM.",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel -d dist src --ignore **/*.spec.js",
